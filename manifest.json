{
    "name": "🅱️",
    "description": "Replace the letter B with the 🅱️ emoji everywhere.",
    "version": "1.0",
    "manifest_version": 3,
    "background": {"service_worker": "background.js"},
    "action": {},
    "content_scripts": [{
        "matches": ["*://*/*"],
        "js": ["mutation-summary.js", "main.js"],
        "all_frames": true,
        "run_at": "document_start"
    }],
    "permissions": ["storage", "tabs"],
    "icons": {"128": "./assets/b.png"}
}
