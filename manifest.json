{
    "name": "xkcd Night Shift",
    "description": "Reduce the intensity of opinions online!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {"service_worker": "background.js"},
    "action": {},
    "content_scripts": [{
        "matches": ["*://*/*"],
        "js": ["mutation-summary.js", "main.js"],
        "all_frames": true,
        "run_at": "document_start"
    }],
    "permissions": ["storage", "tabs"],
    "icons": {"128": "icon.png"}
}
